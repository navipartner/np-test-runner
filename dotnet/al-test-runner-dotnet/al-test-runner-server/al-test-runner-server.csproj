<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>NaviPartner.ALTestRunner.Server</RootNamespace>
    <InvariantGlobalization>true</InvariantGlobalization>
    <PublishAot>true</PublishAot>
    <EnableDefaultJsonSerializationContext>true</EnableDefaultJsonSerializationContext>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\al-test-runner-dotnet\al-test-runner-dotnet.csproj" />
  </ItemGroup>

  <ItemGroup>
    <JsonSerializerContext Include="NaviPartner.ALTestRunner.Server.Requests.InvokeALTestsRequest" />
  </ItemGroup>

  <ItemGroup>
    <JsonSerializable Include="NaviPartner.ALTestRunner.Server.Requests.InvokeALTestsRequest" />
  </ItemGroup>

</Project>
